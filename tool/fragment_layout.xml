<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools">

    <android.support.constraint.ConstraintLayout
            android:id="@+id/vc1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <ImageView
                android:id="@+id/vi2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="fitXY"/>
        <TextView
                android:id="@+id/vt3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@{textColor}"
                android:textSize="@{textSize}"
                tools:text="demodemo"/>

        <ImageView
                android:id="@+id/vi4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="fitXY"/>
        <ImageView
                android:id="@+id/vi5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="fitXY"/>
        <RelativeLayout
                android:id="@+id/vr6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

            <ImageView
                    android:id="@+id/vi12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"/>
            <ImageView
                    android:id="@+id/vi13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"/>
        </RelativeLayout>

        <TextView
                android:id="@+id/vt7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@{textColor}"
                android:textSize="@{textSize}"
                tools:text="demodemo"/>

        <LinearLayout
                android:id="@+id/vl8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            <TextView
                    android:id="@+id/vt14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@{textColor}"
                    android:textSize="@{textSize}"
                    tools:text="demodemo"/>

            <TextView
                    android:id="@+id/vt15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@{textColor}"
                    android:textSize="@{textSize}"
                    tools:text="demodemo"/>

        </LinearLayout>

        <LinearLayout
                android:id="@+id/vl9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            <TextView
                    android:id="@+id/vt16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@{textColor}"
                    android:textSize="@{textSize}"
                    tools:text="demodemo"/>

            <TextView
                    android:id="@+id/vt17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@{textColor}"
                    android:textSize="@{textSize}"
                    tools:text="demodemo"/>

        </LinearLayout>

        <RelativeLayout
                android:id="@+id/vr10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

            <ImageView
                    android:id="@+id/vi18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"/>
        </RelativeLayout>

        <ImageView
                android:id="@+id/vi11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="fitXY"/>
    </android.support.constraint.ConstraintLayout>

</layout>
    